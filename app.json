{
  "expo": {
    "name": "Sadaa Air",
    "slug": "Sadaa Air",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon-splash.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": false
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/icon-splash.png",
        "backgroundColor": "#255257"
      }
    },

    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/icon-splash.png",
          "imageWidth": 50,
          "resizeMode": "contain",
          "backgroundColor": "#255257"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
